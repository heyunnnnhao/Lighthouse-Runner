{
  "name": "@jd/lighthouse-runner",
  "version": "1.2.10",
  "description": "An opinionated, custom Lighthouse runner for performance testing",
  "author": "heyunhao1",
  "main": "./dist/index.js",
  "license": "ISC",
  "type": "commonjs",
  "keywords": [
    "lighthouse",
    "performance",
    "testing"
  ],
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production node esbuild.js",
    "test": "cross-env NODE_ENV=development node esbuild.js && cross-env NODE_ENV=development node test/lighthouse.js",
    "deploy": "npm run build && npm publish"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.21"
  },
  "dependencies": {
    "lighthouse": "9.2.0",
    "lodash.get": "^4.4.2",
    "lodash.isnumber": "^3.0.3",
    "lodash.mergewith": "^4.6.2"
  }
}
