{
  "name": "@jd/lighthouse-runner",
  "version": "1.3.0",
  "description": "An opinionated, custom Lighthouse runner for performance testing",
  "author": "heyunhao1",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "license": "ISC",
  "type": "commonjs",
  "keywords": [
    "lighthouse",
    "performance",
    "testing"
  ],
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc && node esbuild.js",
    "test": "rm -rf ./results && npm run build && ts-node test/lighthouse.ts",
    "te": "ts-node test/lighthouse.ts",
    "deploy": "npm run build && npm publish"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.21"
  },
  "dependencies": {
    "html-to-text": "^8.1.0",
    "lighthouse": "9.2.0",
    "lodash": "latest"
  }
}
